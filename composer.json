{
    "name": "exs/normalized-url-bundle",
    "type": "symfony-bundle",
    "description": "Bundle for looking up and storing normalized urls in 3 parts truncated at 255 characters each",
    "keywords": ["logging", "bundle", "url", "path", "host", "querystring", "hash", "normalized"],
    "homepage": "https://github.com/ExSituMarketing/EXS-NormalizedUrlBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Weiss",
            "email": "charles@ex-situ.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.7",
        "symfony/framework-bundle": "~2.7",
        "doctrine/orm": "~2.4",
        "swiftmailer/swiftmailer": "~5.4"
    },
    "autoload": {
        "psr-0": { "EXS\\NormalizedUrlBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "EXS/NormalizedUrlBundle"
}
